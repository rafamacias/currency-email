<md-chips ng-model="vm.user.rates" md-autocomplete-snap md-require-match>
            <md-autocomplete
                md-selected-item="vm.selectedItem"
                md-search-text="vm.searchText"
                md-items="item in vm.querySearch(vm.searchText)"
                md-item-text="item.symbol"
                placeholder="Search for a vegetable">
                    <span md-highlight-text="vm.searchText">{{item.symbol}}</span>
            </md-autocomplete>
            <md-chip-template>
                <span>
                    <strong>{{$chip.symbol}}</strong>
                    
                    
                </span>
            </md-chip-template>
        </md-chips>



        

    /**
     * Search for vegetables.
     */
    querySearch (query) {
        if (query) {
            let uppercaseQuery = angular.uppercase(query);
            console.log('this.rates: ' + this.rates + ' query: ' + uppercaseQuery);

            let result = query ? this.rates.filter(this._createFilterFor(uppercaseQuery)) : [];
            
            return result;
        }
    }
    /**
     * Create filter function for a query string
     */
    _createFilterFor(query) {
        let uppercaseQuery = angular.uppercase(query);

        let rates = this.rates;

        return function filterFn(rate) {
            return (rate.symbol.indexOf(uppercaseQuery) === 0);
        };
    }